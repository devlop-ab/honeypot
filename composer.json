{
    "name": "devlop/honeypot",
    "description": "Simple honeypot made for Laravel FormRequest",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Rosenson",
            "email": "johan@devlop.se"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "illuminate/collections": "^7.0|^8.0",
        "illuminate/container": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Devlop\\Honeypot\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Devlop\\Honeypot\\HoneypotServiceProvider"
            ]
        }
    }
}
